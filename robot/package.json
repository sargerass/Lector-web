{
  "name": "frontend-skeleton-webpack",
  "version": "1.0.0",
  "description": "A webpack boilerplate for building web apps",
  "main": "./app/scripts/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "analyze": "webpack-bundle-analyzer ./stats.json",
    "build:optimize": "webpack --env.NODE_ENV=production --env.ENABLE_OPTIMIZE_IMAGES --progress --display-modules",
    "build": "webpack --env.NODE_ENV=production --progress --display-modules --colors",
    "build:opt_onebundle": "webpack --env.NODE_ENV=production --env.ENABLE_OPTIMIZE_IMAGES --env.GENERATE_ONE_JS_BUNDLE --progress --display-modules --colors",
    "build:onebundle": "webpack --env.NODE_ENV=production --env.GENERATE_ONE_JS_BUNDLE --progress --display-modules --colors",
    "start:prod": "webpack-dev-server --env.NODE_ENV=production --env.ENABLE_OPTIMIZE_IMAGES --progress --display-modules --colors",
    "start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --open --hot --progress --display-modules --colors --env.NODE_ENV=development --env.ENABLE_LINT\"",
    "start:lintless": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --open --hot --progress --display-modules --colors --env.NODE_ENV=development\"",
    "debug": "node --inspect-brk ./node_modules/webpack/bin/webpack.js --env.NODE_ENV=production --env.ENABLE_OPTIMIZE_IMAGES --env.GENERATE_ONE_JS_BUNDLE --progress --display-modules --colors",
    "django:dev": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env.NODE_ENV=development --env.ENABLE_LINT --hot --progress --display-modules --colors\"",
    "django:prod": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env.NODE_ENV=production --progress --display-modules --colors\"",
    "stylelint-check": "stylelint-config-prettier-check"
  },
  "keywords": [
    "boilerplate",
    "webpack",
    "pug",
    "jade",
    "sass",
    "scss"
  ],
  "author": "Dennis Huillca Portilla",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.10",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-compat": "^2.7.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "expose-loader": "^0.7.5",
    "fast-sass-loader": "^1.4.7",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.18.10",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "pug-html-loader": "^1.1.5",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "require-yml": "^1.3.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.2.1",
    "stylelint-prettier": "^1.0.6",
    "stylelint-scss": "^3.5.4",
    "stylelint-webpack-plugin": "^0.10.5",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "webpack-stats-plugin": "^0.2.1",
    "webpack4-bundle-tracker": "0.0.1-beta"
  },
  "browserslist": [
    "last 2 versions",
    "ie 8",
    "ie 9",
    "> 1%",
    "Firefox >= 20",
    "Opera 12.1",
    "iOS 7"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "jquery-validation": "^1.19.0",
    "jquery.alphanum": "^1.0.26",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "styled-components": "^4.2.0"
  }
}
